{
  "name": "@mistweaverco/cheza.js-default-ui-styles",
  "version": "1.0.0",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "dev": "sass --watch src/styles.scss:dist/styles.css",
    "build": "sass --no-source-map --style=compressed --load-path=node_modules src/styles.scss dist/styles.css"
  }
}
